{
  "manifest_version": 3,
  "name": "ClueLy Killer Test",
  "version": "1.0",
  "description": "One-button extension to hit /kill on localhost:8080",
  "host_permissions": [
    "http://localhost:8080/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Kill ClueLy"
  },
  "permissions": [
    "downloads"
  ]
}
